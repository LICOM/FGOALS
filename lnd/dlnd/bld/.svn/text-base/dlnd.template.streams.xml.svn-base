<stream datasource="RUNOFF.RX1">
   <comment>
      Stream description file for Dai-Trenberth 1 degree runoff
   </comment>
   <dataSource>
      NCAR OCE
      </dataSource>
   <domainInfo>
      <variableNames>
         time          time
         xc            lon
         yc            lat
         area          area
         mask          mask
      </variableNames>
      <filePath>
         $DIN_LOC_ROOT/lnd/dlnd7/RX1
      </filePath>
      <fileNames> runoff.daitren.annual.090225.nc </fileNames>
   </domainInfo>
   <fieldInfo>
      <variableNames>
         runoff roff
      </variableNames>
      <filePath>
         $DIN_LOC_ROOT/lnd/dlnd7/RX1
      </filePath>
      <fileNames> runoff.daitren.annual.090225.nc </fileNames>
   </fieldInfo>
</stream>
